+icons.path = $${PREFIX}/share/icons
+icons.files = images/save.png
+INSTALLS += icons
+
diff --git a/window.desktop b/window.desktop
new file mode 100644
index 0000000..9192e3c
--- /dev/null
+++ b/window.desktop
@@ -0,0 +1,9 @@
