diff --git a/main.pro b/main.pro
index 998ad68..eaf6ec6 100755
--- a/main.pro
+++ b/main.pro
@@ -16,3 +16,14 @@ RESOURCES += application.qrc
 LIBS += -lhunspell-1.7
 
 INCLUDEPATH += /usr/share/hunspell
+
+BINDIR = $$PREFIX/bin
+DATADIR = $$PREFIX/share
+target.path = $$BINDIR
+desktop.files =window.desktop
+desktop.path = $$DATADIR/applications/
